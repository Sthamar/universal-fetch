{
  "name": "universal-fetch",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
  "build": "rm -rf dist && rollup -c",
  "dev": "vite",
  "test": "vitest",
  "clean": "rm -rf dist"
},
  "peerDependencies": {
    "svelte": ">=4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/react": "^18.0.0",
    "rollup": "^4.50.0",
    "rollup-plugin-dts": "^6.2.3",
    "svelte": "^4.0.0",
    "typescript": "^5.9.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./react": {
      "import": "./dist/react/index.esm.js",
      "require": "./dist/react/index.cjs.js"
    },
    "./svelte": {
      "import": "./dist/svelte/index.esm.js",
      "require": "./dist/svelte/index.cjs.js"
    }
  }

}
